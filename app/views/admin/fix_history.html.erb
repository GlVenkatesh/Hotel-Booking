
<%= render 'navadmin' %>
<div class="fix-table">
<table class="table table-striped table-hover">
                         
<thead class="thead-light">
  <tr>
  
    <th scope="col">Guest </th>
    <th scope="col">Email</th>
    <th scope="col">Room_No</th>
    <th scope="col">Price</th>
    <th scope="col">Hotel</th>
    <th scope="col">City</th>
    <th scope="col">check_in_date </th>
    <th scope="col">check_out_date</th>
  </tr>
</thead>
 
<tbody>
<% @fix_history.each do |f| %> 
<tr>
  
    <td><%= f.user.name %></td> 
    <td><%= f.user.email %></td>
    <td><%= f.room.no %></td>
    <td><%= f.total_price %></td>
    <td><%= f.room.hotel.name %></td>
    <td><%= f.room.hotel.city.name %></td>
    <td><%= f.fix.check_in %></td>
    <td><%= f.fix.check_out %></td> 
</tr>
<% end %>                   
</tbody>     

   
  
  
</table>
</div>
<div class="fix-his">
<%= link_to "admin Home" ,admin_home_path,:class=> "btn btn-warning text-dark" %>
</div>